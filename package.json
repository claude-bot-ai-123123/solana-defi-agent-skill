{
  "name": "@openclaw/solana-blinks",
  "version": "1.0.0",
  "description": "Production-ready Solana Blinks CLI with full Dialect Standard Blinks Library (SBL) coverage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "blinks": "./dist/cli.js",
    "solana-blinks": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "blinks",
    "dialect",
    "defi",
    "ai-agent",
    "openclaw",
    "kamino",
    "marginfi",
    "jupiter",
    "raydium",
    "orca",
    "meteora",
    "drift",
    "lulo"
  ],
  "author": "OpenClaw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/solana-blinks-skill.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.6",
    "bs58": "^5.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "SKILL.md"
  ]
}
